---
permalink: feed/reading.xml
excludeFromSitemap: true
---
<?xml version="1.0" encoding="UTF-8"?><rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
	>

<channel>
	<title>justmarkup is reading</title>
	<atom:link href="https://justmarkup.com/feed/reading.xml" rel="self" type="application/rss+xml" />
	<link>https://justmarkup.com/reading/</link>
	<description></description>
	<lastBuildDate>{{ collections.all | rssLastUpdatedDate }}</lastBuildDate>
	<language>en-US</language>
	<sy:updatePeriod>hourly</sy:updatePeriod>
	<sy:updateFrequency>1</sy:updateFrequency>
		{%- for entry in pocket | reverse %}						
      <item>
        <title>{{ entry.resolved_title }}</title>
        <link>{{ entry.given_url }}</link>
        <pubDate>{{ entry.time_added | dateFromPocket }}</pubDate>
        <guid isPermaLink="false">{{ entry.given_url  }}</guid>
        <description>{{ entry.excerpt }}</description>
      </item>
		{%- endfor %}
	</channel>
</rss>
