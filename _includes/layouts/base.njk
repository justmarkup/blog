<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, viewport-fit=cover">
    <title>{{ renderData.title or title or metadata.title }}</title>
    
    <meta name="theme-color" content="#089b52">
    <link rel="manifest" href="/manifest.webmanifest">
    <link rel="icon" type="image/png" href="/img/icons/icon-128x128.png" sizes="128x128" />
    <link rel="apple-touch-icon" sizes="152x152" href="/img/icons/icon-152x152.png" />
    <link rel="webmention" href="https://webmention.io/justmarkup.com/webmention" />
    <link rel="pingback" href="https://webmention.io/justmarkup.com/xmlrpc" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="justmarkup">
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    {% if date %}
      <meta name="author" content="Michael Scharnagl" />
      <meta name="robots" content="index,follow" />
      <meta property="og:type" content="article" />
      <meta property="article:author" content="Michael Scharnagl" />
      <meta name="twitter:card" content="summary" />
      {% if ogImage %}
        <meta name="twitter:image" property="og:image" content="{{ ogImage }}" />
      {% endif %}
      {% if ogImageAlt %}
        <meta name="twitter:image:alt" property="og:image:alt" content="{{ ogImageAlt }}" />
      {% endif %}
      <meta name="twitter:site" content="@justmarkup" />
      <meta name="twitter:site:id" content="20070025" />
      <meta name="twitter:creator" content="@justmarkup" />
      <meta name="twitter:creator:id" content="20070025" />
      <meta name="twitter:url" property="og:url" content="{{ page.url }}" />
      <meta name="twitter:title" property="og:title" content="{{ renderData.title or title or metadata.title }}" />
      <meta name="twitter:description" property="og:description" content="{{ renderData.description or description or metadata.description }}" />
    {% endif %}
    {% set css %}{% include "main.css" %}{% endset %}
    <style>{{ css | cssmin | safe }}</style>
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/rss+xml" title="{{ metadata.title }}">
    <script defer src="/js/main.js"></script>
  </head>
  <body {% if templateClass %} class="{{ templateClass }}"{% endif %}>
    <a href="#main" id="skiplink" class="skiplink">Skip to main content</a>
    <header id="header">
      <a class="logo" href="{{ '/' | url }}">
